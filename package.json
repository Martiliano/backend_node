{
  "name": "boilerplate-api",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Martiliano Rodrigo Panosso <martilianopanosso@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "sucrase-node src/main/server.ts",
    "start": "node dist/src/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "rimraf dist && tsc -p tsconfig-build.json -w",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:staged": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache -- --findRelatedTests",
    "test:verbose": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache -- --watch -c jest-unit-config.js",
    "test:integration": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache -- --watch -c jest-integration-config.js",
    "test:ci": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache -- --coverage",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info",
    "check": "yarn-check -s -u"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/filesystem": "^0.0.32",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mongodb": "^3.6.20",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "coveralls": "^3.1.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.2.6",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.0",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.0.1",
    "multer": "^1.4.3",
    "node-html-parser": "^5.2.0",
    "puppeteer": "^10.2.0",
    "swagger-ui-express": "^4.1.6",
    "validator": "^13.6.0"
  }
}
